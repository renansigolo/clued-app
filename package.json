{
  "name": "clued-app",
  "version": "0.0.1",
  "description": "CluedTv App",
  "author": "8D Digital",
  "homepage": "https://8d.digital/",
  "private": true,
  "scripts": {
    "clean": "rm -rf www node_modules platforms plugins package-lock.json .sourcemaps",
    "clean:install": "npm run clean && npm i && ionic cordova prepare",
    "verify": "~/Library/Android/sdk/build-tools/29.0.3/zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk cluedtv.apk",
    "sign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore cluedtv-release-key.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk clued_tv",
    "build": "ionic build",
    "build:web": "ionic build --prod",
    "build:prod": "ionic build --prod && ionic cordova build ios --prod && ionic cordova build android --prod --release",
    "build:dev": "ionic build && ionic cordova build ios && ionic cordova build android",
    "build:ios:prod": "ionic cordova build ios --prod",
    "build:ios": "ionic cordova prepare ios",
    "build:android:prod": "ionic cordova build android --prod --release && npm run sign && npm run verify",
    "build:android": "ionic cordova prepare android",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "npm run build:web && firebase deploy -P production",
    "ios": "ionic cordova run ios --consolelogs -l --emulator --native-run --connect --address=0.0.0.0",
    "android": "ionic cordova run android --debug --consolelogs -l",
    "start": "ionic serve -o",
    "prettier": "prettier --config ./.prettierrc.json --write \"./**/*.{js,ts,json,md,scss,component.html,page.html}\"",
    "lint:ts:fix": "ng lint --format stylish --fix",
    "lint:ts": "ng lint --format stylish",
    "lint:fix": "npm run lint:ts:fix",
    "test": "npm run lint:ts"
  },
  "dependencies": {
    "@angular/common": "~9.1.7",
    "@angular/core": "~9.1.7",
    "@angular/fire": "^6.0.0",
    "@angular/forms": "~9.1.7",
    "@angular/platform-browser": "~9.1.7",
    "@angular/platform-browser-dynamic": "~9.1.7",
    "@angular/router": "~9.1.7",
    "@capacitor/android": "^2.4.7",
    "@capacitor/cli": "^2.4.7",
    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@ionic-native/core": "^5.26.0",
    "@ionic-native/device": "^5.26.0",
    "@ionic-native/splash-screen": "^5.26.0",
    "@ionic-native/status-bar": "^5.26.0",
    "@ionic-native/stripe": "^5.26.0",
    "@ionic/angular": "^5.1.1",
    "@ionic/storage": "^2.2.0",
    "@vimeo/player": "^2.11.0",
    "cordova-android": "^8.1.0",
    "cordova-ios": "^5.1.1",
    "cordova-plugin-device": "2.0.3",
    "cordova-plugin-ionic-webview": "^4.2.1",
    "cordova-plugin-splashscreen": "5.0.3",
    "cordova-plugin-statusbar": "2.4.3",
    "cordova-plugin-whitelist": "1.3.4",
    "cordova-sqlite-storage": "^5.0.0",
    "core-js": "^3.6.5",
    "firebase": "^7.14.4",
    "rxjs": "~6.5.5",
    "tslib": "^1.13.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.901.6",
    "@angular-devkit/build-angular": "~0.901.6",
    "@angular-devkit/core": "~9.1.6",
    "@angular-devkit/schematics": "~9.1.6",
    "@angular/cli": "~9.1.6",
    "@angular/compiler": "~9.1.7",
    "@angular/compiler-cli": "~9.1.7",
    "@angular/language-service": "~9.1.7",
    "@ionic/angular-toolkit": "^2.2.0",
    "@types/node": "^12.12.39",
    "@types/stripe-v3": "^3.1.17",
    "codelyzer": "^5.2.2",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "prettier": "^2.0.5",
    "ts-node": "~8.6.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": ">= 7.0.2",
    "yarn": "NO LONGER USED - Please use npm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm t && git add ."
    }
  },
  "lint-staged": {
    "./**/*.{js,ts,json,md,scss,component.html}": [
      "prettier --config ./.prettierrc.json --write"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-device": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-whitelist": {},
      "cordova-sqlite-storage": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
